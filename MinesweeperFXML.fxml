<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" fillHeight="false" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mines.Controller">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="140.0" prefWidth="130.0">
         <children>
            <Button fx:id="reset" alignment="CENTER" mnemonicParsing="false" onAction="#resetAction" prefHeight="25.0" prefWidth="100.0" text="Reset" />
            <HBox fillHeight="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="102.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="60.0" text="width">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="widthText" prefHeight="25.0" prefWidth="40.0" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="4.0" />
               </padding>
            </HBox>
            <HBox prefHeight="30.0" prefWidth="102.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="60.0" text="height">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="heightText" prefHeight="25.0" prefWidth="40.0" />
               </children>
               <padding>
                  <Insets top="4.0" />
               </padding>
            </HBox>
            <HBox prefHeight="30.0" prefWidth="102.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="60.0" text="mines">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="minesText" prefHeight="25.0" prefWidth="40.0" />
               </children>
               <padding>
                  <Insets top="4.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
